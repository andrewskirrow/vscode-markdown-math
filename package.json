{
    "name": "markdown-katex",
    "displayName": "Markdown Katex",
    "description": "Adds the markdown-it-katex plugin.",
    "version": "0.0.1",
    "publisher": "koehlma",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [ ],
    "main": "./out/src/extension",
    "contributes": {
        "markdown.markdownItPlugins": true,
        "markdown.previewStyles": [
            "./node_modules/katex/dist/katex.min.css"
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "markdown-it-katex": "^2.0.3"
    }
}